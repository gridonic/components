{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-unused-expression": false,
    "max-classes-per-file": [ false ],
    "typedef": [
      true,
      "call-signature",
      "parameter",
      "member-variable-declaration"
    ],
    "variable-name": [
      "allow-leading-underscore"
    ]
  },
  "rulesDirectory": []
}